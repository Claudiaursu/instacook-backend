{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/cooking/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/cooking/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/cooking",
  "projects": {
    "cooking": {
      "type": "application",
      "root": "apps/cooking",
      "entryFile": "main",
      "sourceRoot": "apps/cooking/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cooking/tsconfig.app.json"
      }
    },
    "user-interaction": {
      "type": "application",
      "root": "apps/user-interaction",
      "entryFile": "main",
      "sourceRoot": "apps/user-interaction/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-interaction/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "competitions": {
      "type": "application",
      "root": "apps/competitions",
      "entryFile": "main",
      "sourceRoot": "apps/competitions/src",
      "compilerOptions": {
        "tsConfigPath": "apps/competitions/tsconfig.app.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    },
    "feed": {
      "type": "application",
      "root": "apps/feed",
      "entryFile": "main",
      "sourceRoot": "apps/feed/src",
      "compilerOptions": {
        "tsConfigPath": "apps/feed/tsconfig.app.json"
      }
    }
  }
}